---
import { HowToApply as HowToApplyComponent } from "@/components/pages/howToApply/HowToApply";

import { languages } from "@/i18n/ui";
import Layout from "@/layouts/layout.astro";

export async function getStaticPaths() {
  return Object.keys(languages).map((lang) => ({
    params: { lang },
  }));
}

const lang = Astro.params.lang as keyof typeof languages;
---

<Layout title={"របៀបដាក់ពាក្យ-How To Apple"} lang={lang}>
  <HowToApplyComponent client:load/>
</Layout>
